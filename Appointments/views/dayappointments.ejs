<%- include("partials/header") -%>

  <div class="box" id="heading">
    <h1>Appointments on <%= oneDayData.dateTxt %></h1>
  </div>

  <div class="box">

    <form class="" action="/appointments" method="post">

      <% for (i=0; i<oneDayData.appointments.length; i++) { %>
        <div class="app-item">
          <p class="app-cell"><%= oneDayData.appointments[i].startTxt %> - <%= oneDayData.appointments[i].endTxt %></p>
          <p class="app-cell-free"><%= oneDayData.appointments[i].persnoNo %></p>
          <p class="app-cell-free"><%= oneDayData.appointments[i].duration %></p>
          <p class="app-cell-free"><%= oneDayData.appointments[i].no %></p>
          <button type="submit" name="delete" value="<%= oneDayData.appointments[i].no %>">Delete</button>
        </div>
      <% } %>

    </form>

  </div>


<%- include("partials/footer") -%>
